<?xml version="1.0" encoding="ISO-8859-1" ?>
<scenario>

<counter>
  <counterdef name="session-counter" init="0"> </counterdef>
</counter>

<correlation>
  <channel name="channel-1">
    <retrieve>
      <search-in-map name="session-method-open-id"></search-in-map>
    </retrieve>

    <command name="C1">
      <post-action>
        <insert-in-map channel="channel-1" name="default-session-id"></insert-in-map>
      </post-action>
    </command>

  </channel>
</correlation>

<traffic>
  <send channel="channel-1" label="C1">
    <action>
      <open args="mode=client;dest=127.0.0.1:12345"></open>
    </action>
    <request name="Get-Time"> </request>
   </send>

  <receive channel="channel-1">
    <action>
      <open args="mode=client;dest=127.0.0.1:12345"></open>
    </action>
  
  </receive>

</traffic>

</scenario>









